package day1;

public class Test5 {

	public static void main(String[] args) {
		// type 논리형
		boolean b = true; // or false
//		Boolean c = null; 
		String str = "1";
		// If str 이 null 이면 입력해주세요!
		// 아니면 str 을 출력한다.
		// if (조건) {조건이 맞으면 실행}
		if(str == null) {
			System.out.println("입력해주세요.");
		}
		if (str != null) {
			System.out.println(str);
		}
		
		// 만약 5/5가 0이면 ㅎㅎ --> if
		// 만약 5/5가 8이면 ㅂㅂ --> else if (if로 쓰면 else가 실행될 수 있음)
		// 아니면 ㅋㅋㅋ --> else
		int x = 5;
		
		
		if (x/x != 0) {
			System.out.println("ㅎㅎ");
		} if (x/x == 8) {
			System.out.println("ㅂㅂ");
		} else {
			System.out.println("ㅋㅋㅋ");
		}
	}

}
